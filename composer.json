{
    "name": "mangati/zenvia-notifier",
    "type": "symfony-notifier-bridge",
    "description": "Mangati Zenvia Notifier Bridge",
    "keywords": ["sms", "zenvia", "notifier"],
    "homepage": "https://github.com/mangati/zenvia-notifier",
    "license": "MIT",
    "authors": [
        {
            "name": "Rogerio Lino",
            "email": "rogeriolino@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/http-client": "^4.3|^5.0|^6.0",
        "symfony/notifier": "^5.3|^6.0",
        "symfony/event-dispatcher-contracts": "^1.1|^2|^3"
    },
    "autoload": {
        "psr-4": { "Mangati\\Notifier\\Zenvia\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/phpunit-bridge": "6.1.x-dev"
    }
}
